            var socialbar_width = $('.fb-comments').parent().css('width');
            $('.fb-comments').attr('data-width', socialbar_width);
            $(window).resize(function() {
                var socialbar_width = $('.fb-comments').parent().css('width');
                $('.fb-comments').attr('data-width', socialbar_width);
            });
            
