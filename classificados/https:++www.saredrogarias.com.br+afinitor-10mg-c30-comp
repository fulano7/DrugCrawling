<script src="https://www.saredrogarias.com.br/loja/view/temas/saredrogarias/lib/jquery/jquery.easyui.min.js" type="text/javascript"></script>
 
<script src="https://www.saredrogarias.com.br/loja/view/temas/saredrogarias/plugins/autocomplete/js/jquery-ui-1.10.4.custom.js"></script>
 
 
<script src="https://www.saredrogarias.com.br/loja/view/temas/saredrogarias/lib/personal-bootstrap/js/bootstrap.min.js"></script>
 
<script src="https://www.saredrogarias.com.br/loja/view/temas/saredrogarias/lib/gleen-webfonts/gleen-webfonts.js" type="text/javascript"></script>
 
<script src="https://www.saredrogarias.com.br/loja/view/temas/saredrogarias/js/main.js" type="text/javascript"></script>
<script src="https://www.saredrogarias.com.br/loja/view/temas/saredrogarias/js/mask.js"></script>
 
<script src="https://www.saredrogarias.com.br/loja/view/temas/saredrogarias/js/angular.js"></script>
<script src="https://www.saredrogarias.com.br/loja/view/temas/saredrogarias/js/app.js" type="text/javascript"></script>
<script src="https://www.saredrogarias.com.br/loja/view/temas/saredrogarias/js/zoom.js" type="text/javascript"></script>
<script type="text/javascript">
        jQuery(document).ready(function($) {
             $(window).load(function() {
                setInterval( function(){  
                    // alert('ok');       
                    $(".autocomplete").css("display", "block");         
                }, 50);
            });
            $('.enviar-callback').click(function(){
                // pega os dados
                var nome = $('#nome-callback').val();
                var telefone = $('#telefone-callback').val();
                var email = $('#email-callback').val();

                // validacao
                if (nome == '' || nome.length < 3) {
                    alert('Prencha o nome corretamente');
                    $('#nome-callback').focus();
                    return false;
                }
                if (telefone == '' || telefone.length < 13) {
                    alert('Prencha o telefone corretamente');
                    $('#telefone-callback').focus();
                    return false;
                }
                // chama o ajax
                $.ajax({
                    url: "https://www.saredrogarias.com.br/ajaxEnviarCallBack",
                    type: "POST",
                    data: {
                        nome:nome,
                        email:email,
                        telefone:telefone
                    },
                    dataType: "json",
                    beforeSend: function(){
                        $('#body').prepend('<div id="loading"><img src="https://www.saredrogarias.com.br/loja/view/temas/saredrogarias/img/load.gif" alt="loading" /></div>');
                    },
                    success: function(result){
                        $('#loading').remove();
                    
                        var nome = $('#nome-callback').val(" ");
                        var telefone = $('#telefone-callback').val(" ");
                        var email = $('#email-callback').val(" ");

                        alert('Solicitação enviada com sucesso. Em breve entraremos em contato.');

                        $('#ModalCallBack').modal('hide');

                        
                    },
                    error: function(){
                        $('#loading').remove();
                    }
                });
            });

            $(".inputTelefone").mask("(99) 9999-99999");
            // ZOOM 
            $('.galeria a').on('click',function(){
                var thumb = $(this).attr('href');
                $('.imgPrincipal img').attr('src',thumb);
                $(".produtoZoom").elevateZoom({tint:true, tintColour:'#FFF', tintOpacity:0.5});
                return false;
            });
            $(".produtoZoom").elevateZoom({tint:true, tintColour:'#FFF', tintOpacity:0.5});
             
            $('#btn-login').click(function(event) {
                $('#login-modal').modal('show');
            });
            
            $('#btn-cadastrar').click(function(event) {
                $('#cadastrar-modal').modal('show');
            });

             $('#report-problem').click(function(event) {
                $('#report-problem-modal').modal('show');
            });

            $('#esqueceu-senha-link').click(function(event) {
                $('#login-box').slideUp('slow');
                $('#esqueceu-login-box').slideDown('slow');
                $('.logar-login-btn').css('display','none');
                $('.esqueceu-login-btn').css('display','inline-block');
                $('#title-login').css('display','none');
                $('#title-relembrar').css('display','block');
            });

            $('.enviar-newsletter').click(function(){
                var email = $('.email-newsletter').val();
                alert(email);

            })

            $('.cadastrar-modal-btn').click(function(){
                var tipoPessoa = $('input[name="pessoa"]:checked').val();

                if(tipoPessoa == 0){
                    var nome_razao = $('#nome-cadastrar').val();
                    var cpf_cnpj = $('#cpf-cadastrar').val();

                    if(nome_razao == ''){
                        alert('Informe seu Nome antes de continuar.');
                        return false;
                    }

                    if(cpf_cnpj == ''){
                        alert('Informe seu CPF antes de continuar.');
                        return false;
                    }
                }else{
                    var nome_razao = $('#razao-cadastrar').val();
                    var cpf_cnpj = $('#cnpj-cadastrar').val();

                    if(nome_razao == ''){
                        alert('Informe sua Razão Social antes de continuar.');
                        return false;
                    }

                    if(cpf_cnpj == ''){
                        alert('Informe seu CNPJ antes de continuar.');
                        return false;
                    }
                }

                var email = $('#email-cadastrar').val();
                var dataNascimento = $('#data-cadastrar').val();
                var telefone = $('#telefone-cadastrar').val();
                var celular = $('#celular-cadastrar').val(); 

                if(email == ''){
                    alert('Informe seu email antes de continuar.');
                    return false;
                }

                if(telefone == ''){
                    alert('Informe seu telefone antes de continuar.');
                    return false;
                }

                $.ajax({
                    url: "https://www.saredrogarias.com.br/ajaxCadastrarCliente",
                    type: "POST",
                    data: {
                        tipoPessoa:tipoPessoa,
                        nome_razao:nome_razao,
                        cpf_cnpj:cpf_cnpj,
                        dataNascimento:dataNascimento,
                        telefone:telefone,
                        celular:celular,
                        email:email
                    },
                    dataType: "json",
                    beforeSend: function(){
                        $('#body').prepend('<div id="loading"><img src="https://www.saredrogarias.com.br/loja/view/temas/saredrogarias/img/load.gif" alt="loading" /></div>');
                    },
                    success: function(result){
                        $('#loading').remove();
                        if(result.result == 'success'){
                            $('#loading').remove();

                            $('.dados-cadastrais').slideUp('slow');
                            $('.sucesso').slideDown('slow');

                            alert('Cadastro efetuado com sucesso, a senha foi enviada para o email correspondente a conta.');

                            $('#cadastrar-modal').modal('hide');

                        }else{
                            alert(result.msgm);
                        }
                    },
                    error: function(){
                        alert('O cadastro não foi realizado, entre em contato.');
                        $('#loading').remove();
                    }
                });
            })

            $('.enviar-report').click(function(){

                var nome = $('#nome-report').val();
                var email = $('#email-report').val();
                var assunto = $('#assunto-report').val();
                var mensagem = $('#mensagem-report').val(); 

                if(nome == ''){
                    alert('Informe seu nome antes de continuar.');
                    return false;
                }

                if(email == ''){
                    alert('Informe seu email antes de continuar.');
                    return false;
                }

                if(assunto == ''){
                    alert('Informe o assunto antes de continuar.');
                    return false;
                }

                if(mensagem == ''){
                    alert('Informe o mensagem antes de continuar.');
                    return false;
                }

                $.ajax({
                    url: "https://www.saredrogarias.com.br/ajaxEnviarProblema",
                    type: "POST",
                    data: {
                        nome:nome,
                        email:email,
                        assunto:assunto,
                        mensagem:mensagem
                    },
                    dataType: "json",
                    beforeSend: function(){
                        $('#body').prepend('<div id="loading"><img src="https://www.saredrogarias.com.br/loja/view/temas/saredrogarias/img/load.gif" alt="loading" /></div>');
                    },
                    success: function(result){
                        $('#loading').remove();

                        if(result.result == 'success'){
                            $('#loading').remove();

                            alert('Solicitação enviada com sucesso, agradecemos sua ajuda. Em breve o problema será solucionado.');

                            $('#report-problem-modal').modal('hide');

                        }else{
                            alert(result.msgm);
                        }
                    },
                    error: function(){
                        $('#loading').remove();
                    }
                });
            })

            $('.esqueceu-login-btn').click(function(event) {
                var email = $('#emailEsqueceuLogin').val();

                if(email == ''){
                    alert('Informe seu email antes de continuar.');
                    return false;
                }

                $.ajax({
                    url: "https://www.saredrogarias.com.br/ajaxEnviarSenhaCliente",
                    type: "POST",
                    data: { 
                        email:email
                    },
                    dataType: "json",
                    beforeSend: function(){
                        $('#body').prepend('<div id="loading"><img src="https://www.saredrogarias.com.br/loja/view/temas/saredrogarias/img/load.gif" alt="loading" /></div>');
                    },
                    success: function(result){
                        $('#loading').remove();
                        if(result.result == 'success'){
                            $('#loading').remove();

                            $('#login-box').slideDown('slow');
                            $('#esqueceu-login-box').slideUp('slow');
                            $('.esqueceu-login-btn').css('display','none');
                            $('.logar-login-btn').css('display','inline-block');
                            $('#title-relembrar').css('display','none');
                            $('#title-login').css('display','block');

                            alert('A senha foi enviada para o email correspondente a conta.');

                            $('#login-modal').modal('hide');

                        }else{
                            alert(result.msgm);
                        }
                    },
                    error: function(){
                        alert('Erro ao procurar os dados solicitados');
                        $('#loading').remove();
                    }
                });

            });

            $('#btn-cadastrar-primeira-compra').click(function(event) {
                var email = $('#input-email-primeira-compra').val();

                if(email != "")
                {
                   var filtro = /^([\w-]+(?:\.[\w-]+)*)@((?:[\w-]+\.)*\w[\w-]{0,66})\.([a-z]{2,6}(?:\.[a-z])?)$/i;
                   if(filtro.test(email))
                   {
                     
                   } else {
                     alert('Endereço de email inválido.'); return false;
                   }
                } else {
                     alert('Digite um endereço de email.'); return false;
                }

                $.ajax({
                    url: "https://www.saredrogarias.com.br/ajaxCadastrarEmailPrimeiraCompra",
                    type: "POST",
                    data: { 
                        email:email
                    },
                    dataType: "json",
                    beforeSend: function(){
                        $('#body').prepend('<div id="loading"><img src="https://www.saredrogarias.com.br/loja/view/temas/saredrogarias/img/load.gif" alt="loading" /></div>');
                    },
                    success: function(result){
                        $('#loading').remove();
                        if(result.result == 'success'){
                            $('#loading').remove();

                            alert('Confira a supresa em seu email');
                            $('#descontoPrimeiraCompraModal').modal('hide');

                        }else{
                            alert(result.msgm);
                        }
                    },
                    error: function(){
                        alert('Erro ao procurar os dados solicitados');
                        $('#loading').remove();
                    }
                });

            });

            //cadastrar-modal
            if($('#pessoa-fisica-modal').is(':checked')) {
                $('#juridica-razao-modal, #juridica-cnpj-modal').css('display','none');
                $('#fisica-nome-modal, #fisica-cpf-modal').fadeIn(500);
            }else if($('#pessoa-juridica-modal').is(':checked')) {
                $('#fisica-nome-modal, #fisica-cpf-modal').css('display','none');
                $('#juridica-razao, #juridica-cnpj-modal').fadeIn(500);
            }

            $('#pessoa-fisica-modal').click(function(){
                if($('#fisica-nome-modal, #fisica-cpf-modal').is(':hidden')){
                    $('#juridica-razao-modal, #juridica-cnpj-modal').css('display','none');
                    $('#fisica-nome-modal, #fisica-cpf-modal').fadeIn(500);
                }
            });

            $('#pessoa-juridica-modal').click(function(){
                if($('#juridica-razao-modal, #juridica-cnpj-modal').is(':hidden')){
                    $('#fisica-nome-modal, #fisica-cpf-modal').css('display','none');
                    $('#juridica-razao-modal, #juridica-cnpj-modal').fadeIn(500);
                }
            });

            $('#logar-btn').click(function(){

                var emailLogin = $('#emailLoginHeader').val();
                var senhaLogin = $('#senhaLoginHeader').val();

                if(emailLogin == ''){
                    alert('Informe seu email antes de continuar.');
                    return false;
                }

                if(senhaLogin == ''){
                    alert('Informe sua senha antes de continuar.');
                    return false;
                }

                $.ajax({
                    url: "https://www.saredrogarias.com.br/ajaxLogin",
                    type: "POST",
                    data: { 
                        emailLogin:emailLogin,
                        senhaLogin:senhaLogin
                    },
                    dataType: "json",
                    beforeSend: function(){
                        $('#body').prepend('<div id="loading"><img src="https://www.saredrogarias.com.br/loja/view/temas/saredrogarias/img/load.gif" alt="loading" /></div>');
                    },
                    success: function(result){
                        $('#loading').remove();
                        if(result.result == 'success'){
                            $('#loading').remove();
                            window.location.href = 'https://www.saredrogarias.com.br/minha-conta';
                        }else{
                            alert(result.msgm);
                        }
                    },
                    error: function(){
                        alert('Erro ao procurar os dados solicitados');
                        $('#loading').remove();
                    }
                });
            });

            $('#search-header-btn').click(function(event) {
                var busca =  $('.search-input-header').val();
                window.location.href = 'https://www.saredrogarias.com.br/busca?g='+busca+'';
            });

            $('.search-input-header').keypress(function(e) {
               var tecla = (e.keyCode?e.keyCode:e.which);

               if(tecla == 13){
                    var busca =  $('.search-input-header').val();
                    window.location.href = 'https://www.saredrogarias.com.br/busca?g='+busca+'';
               }

            });


            // $('.search-input-header').autocomplete({
            //     source: function (request, response) {
