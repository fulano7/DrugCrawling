            //           dataType: 'json'
            //         });
            //     },
            //     type: "POST",
            //     minLength: 2,
            //     delay: 0,
            //     select: function( event, ui ) { 
            //         if(ui.item.link) {
            //             window.location.href = ui.item.link;
            //         }
            //     } 
            // }).data("ui-autocomplete")._renderItem = function (ul, item) {
            //      return $("<li></li>")
            //          .data("item.autocomplete", item)
            //          .append("<a>" + item.label + "</a>")
            //          .appendTo(ul);
            //  }; 

        });
        </script>
<script>
            var socialbar_width = $('.fb-comments').parent().css('width');
            $('.fb-comments').attr('data-width', socialbar_width);
            $(window).resize(function() {
                var socialbar_width = $('.fb-comments').parent().css('width');
                $('.fb-comments').attr('data-width', socialbar_width);
            });
            
        </script>
