<script>

//<![CDATA[
           
         
           

jQuery(document).ready(function() {
	
	if(!mobileAndTabletcheck()){
		jQuery("#main_image").addimagezoom({
			zoomrange: [2, 10],
			magnifiersize: [500,400],
			magnifierpos: 'right',
			cursorshade: true,
			cursorshadecolor: 'white',
			cursorshadeopacity: 0.3,
			cursorshadeborder: '2px solid black',
			largeimage: $("#main_image").attr('src')
			});
	}
	
	
	
	

	var title = unescape($("#name").text());
	var description = unescape(encodeURIComponent(document.getElementById("descriptionMeta").textContent));
	var image = document.getElementById("main_image").getAttribute('src');
	var url = document.getElementById("urlMeta").textContent;
	
	jQuery('#share_button_twitter').on('click', function(e)  {
		 
		e.preventDefault();
		var link = "https://twitter.com/intent/tweet?hashtags=VSMSHOP&text="+title+"&tw_p=tweetbutton&url=http://www.vsmshop.com.br"+url+"&via=vsmshop";
		window.open(link, "", "width=800, height=400");
			
	});
	
	jQuery('#share_button_google').on('click', function(e)  {
		 
		e.preventDefault();
		var link = "https://plus.google.com/share?url=http://www.vsmshop.com.br"+url;
		window.open(link, "", "width=800, height=400");
			
	});
	
	jQuery('#share_button_facebook').on('click', function(e)  {
		e.preventDefault();
		var link = "https://www.facebook.com/dialog/feed?app_id=1488350268124762&display=popup&caption=VSM Shop&link=http://www.vsmshop.com.br"+url+"&redirect_uri=http://www.facebook.com/&description="+''+"&picture=http://www.vsmshop.com.br"+image+"&name="+title;
		window.open(link, "", "width=800, height=400");
			
	});
	
	
	jQuery('.tabs .tab-links a').on('click', function(e)  {
        var currentAttrValue = jQuery(this).attr('href');
        // Show/Hide Tabs
        jQuery('.tabs ' + currentAttrValue).show().siblings().hide();
 
        // Change/remove current tab to active
        jQuery(this).parent('li').addClass('active').siblings().removeClass('active');
 
        e.preventDefault();
    });
	

	
});




//Função para atualizar a imagem com zoom.
function trocaImagem(as){
	
	if(as != null){
		$("#main_image").attr("src",as);
	}
	 
	if(!mobileAndTabletcheck()){
	jQuery("#main_image").addimagezoom({
		zoomrange: [2, 10],
		magnifiersize: [500,400],
		magnifierpos: 'right',
		cursorshade: true,
		cursorshadecolor: 'white',
		cursorshadeopacity: 0.3,
		cursorshadeborder: '2px solid black',
		largeimage: as
		});
	}
	
	return false;
}




function goInfo(){
	
	    $('html, body').animate({
	        scrollTop: $("#productDescription").offset().top - 70
	    }, 1000);
	
}

function goProductRel(){
	
    $('html, body').animate({
        scrollTop: $("#productRel").offset().top - 70
    }, 1000);

}

function goAvaliacoes(){
	
    $('html, body').animate({
        scrollTop: $("#div_avaliacao").offset().top - 70
    }, 1000);

}

//]]>
</script>

