            <script type="text/javascript">
        function ajaxsearchsubmit(form) {
            var search = encodeURIComponent(form.w.value);
            document.activeElement.blur();
            window.location = "http://busca.farmagora.com.br/search?w=" + search;
            return false;
        }
    </script>
