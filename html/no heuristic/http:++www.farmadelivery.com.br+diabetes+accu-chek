    <p class="category-image"><img src="http://www.farmadelivery.com.br/media/catalog/category/kitPERF2015_800x170px_1.jpg" alt="Accu-Chek" title="Accu-Chek" /></p>
    <script>
    actualQty = 1;
    function addToQty( intQty, inputId )
    {
        actualQty += intQty;
        actualQty = (actualQty <= 1 ) ? 1 : actualQty;
        document.getElementById(inputId).value=actualQty;
    }
    
    function addToCartFromList(url, qty)
    {
        url+='qty/'+qty;
        setLocation(url);
    }

