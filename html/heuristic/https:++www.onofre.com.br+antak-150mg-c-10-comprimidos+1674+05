            function incrementValue() {
                var a = parseInt(document.getElementById("txt_QtdDP").value, 10);
                a = isNaN(a) ? 0 : a;
                a++;
                if (a == 21) {
                    a = 20
                }
                //if (a < 10) {
                //    a = ("0" + a)
                //}

                document.getElementById("txt_QtdDP").value = a
            }

