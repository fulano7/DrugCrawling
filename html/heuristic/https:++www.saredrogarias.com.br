ype="button" class="btn-modal" id="btn-cadastrar-primeira-compra">Cadastrar</a>\r\n                    </div>\r\n                </div><!-- /.modal-content -->\r\n            </div><!-- /.modal-dialog -->\r\n        </div><!-- /.modal -->\r\n\r\n        <div class="modal fade" id="consultaPedido" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true" style="top:30% !important;">\r\n            <div class="modal-dialog" style="width:1000px !important; ">\r\n                <div class="modal-content">\r\n                    <div class="modal-body">\r\n                        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>\r\n                        <br>\r\n                        <div class="status">\r\n                            <div class="status-content">\r\n                                <div class="status-1 success">\r\n                                    Pedido Realizado\r\n                                </div>\r\n                            </div>\r\n                            <div class="status-content">\r\n                                <div class="status-2 ">\r\n                                    Aguardando Pagamento\r\n                                </div>\r\n                            </div>\r\n                            <div class="status-content">\r\n                                <div class="status-3 ">\r\n                                    Enviado\r\n                                </div>\r\n                            </div>\r\n                            <div class="status-content">\r\n                                <div class="status-4 ">\r\n                                    Entrega\r\n                                </div>\r\n                            </div>\r\n                        </div>\r\n                        <div class="msgm-entrega">\r\n                            <p>O seu pedido foi enviado para o destinat\xc3\xa1rio. </p>\r\n                        </di